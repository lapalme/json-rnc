{"$schema":"http://json-schema.org/draft-07/schema#",
 "definitions":{"Product":{"additionalProperties":false,
                           "properties":{"dimensions":{"additionalProperties":false,
                                                       "properties":{"height":{"type":"number"},
                                                                     "length":{"type":"number"},
                                                                     "width":{"type":"number"}},
                                                       "required":["length","width","height"],
                                                       "type":"object"},
                                         "id":{"type":"integer"},
                                         "name":{"type":"string"},
                                         "price":{"exclusiveMinimum":0,
                                                  "type":"number"},
                                         "tags":{"items":{"type":"string"},
                                                 "type":"array"},
                                         "warehouseLocation":{"$ref":"#/definitions/geo"}},
                           "required":["id","name","price"],
                           "type":"object"},
                "geo":{"additionalProperties":false,
                       "properties":{"latitude":{"type":"number"},
                                     "longitude":{"type":"number"}},
                       "required":["latitude","longitude"],
                       "type":"object"}},
 "items":{"$ref":"#/definitions/Product"},
 "type":"array"}
