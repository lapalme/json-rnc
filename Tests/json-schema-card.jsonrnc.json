{"$schema":"http://json-schema.org/draft-07/schema#",
 "additionalProperties":false,
 "definitions":{"address":{"additionalProperties":false,
                           "properties":{"country-name":{"type":"string"},
                                         "extended-address":{"type":"string"},
                                         "locality":{"type":"string"},
                                         "post-office-box":{"type":"string"},
                                         "postal-code":{"type":"string"},
                                         "region":{"type":"string"},
                                         "street-address":{"type":"string"}},
                           "required":["locality","region"],
                           "type":"object"},
                "geo":{"additionalProperties":false,
                       "properties":{"latitude":{"maximum":90,
                                                 "minimum":-90,
                                                 "type":"number"},
                                     "longitude":{"maximum":180,
                                                  "minimum":-180,
                                                  "type":"number"}},
                       "required":["latitude","longitude"],
                       "type":"object"}},
 "properties":{"additionalName":{"items":{"type":"string"},
                                 "type":"array"},
               "adr":{"$ref":"#/definitions/address"},
               "bday":{"type":"string"},
               "email":{"additionalProperties":false,
                        "properties":{"type":{"type":"string"},
                                      "value":{"type":"string"}},
                        "required":[],
                        "type":"object"},
               "familyName":{"type":"string"},
               "fn":{"type":"string"},
               "geo":{"$ref":"#/definitions/geo"},
               "givenName":{"type":"string"},
               "honoricPrefix":{"items":{"type":"string"},
                                "type":"array"},
               "logo":{"type":"string"},
               "nickname":{"type":"string"},
               "org":{"additionalProperties":false,
                      "properties":{"organizationName":{"type":"string"},
                                    "organizationUnit":{"type":"string"}},
                      "required":[],
                      "type":"object"},
               "photo":{"type":"string"},
               "role":{"type":"string"},
               "sound":{"type":"string"},
               "tel":{"additionalProperties":false,
                      "properties":{"type":{"type":"string"},
                                    "value":{"type":"string"}},
                      "required":[],
                      "type":"object"},
               "title":{"type":"string"},
               "tz":{"type":"string"},
               "url":{"type":"string"}},
 "required":["familyName","givenName"],
 "type":"object"}
